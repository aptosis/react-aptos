{
  "name": "@aptosis/react-aptos-api",
  "version": "0.1.26",
  "description": "Lightweight React hook for connecting to the Aptos blockchain.",
  "repository": "git@github.com:aptosis/react-aptos.git",
  "author": "Ian Macalinao <ian@aptos.is>",
