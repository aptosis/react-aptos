{
  "name": "@aptosis/react-aptos",
  "version": "0.1.16",
  "description": "React library for interacting with the Aptos blockchain.",
  "repository": "git@github.com:aptosis/react-aptos.git",
  "author": "Ian Macalinao <ian@aptos.is>",
  "license": "Apache-2.0",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "keywords": [
    "react",
    "wallet",
    "blockchain",
    "aptos",
    "move"
  ],
  "homepage": "https://aptos.is",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "files": [
    "dist/",
    "src/"
  ],
  "publishConfig": {
    "access": "public"
  },
  "bugs": {
    "url": "https://github.com/aptosis/react-aptos/issues",
    "email": "ian@aptos.is"
  },
  "scripts": {
    "build": "tsc && tsc --project tsconfig.cjs.json",
    "clean": "rm -fr dist/"
  },
  "packageManager": "yarn@3.2.1",
  "lint-staged": {
    "*.{ts,tsx,cjs}": "eslint --cache --fix",
    "*.{json,js,jsx,html,css,md,yml,yaml}": "prettier --write"
  },
  "devDependencies": {
    "@movingco/prelude": "^0.1.51",
    "@saberhq/tsconfig": "^2.0.2",
    "@types/react": "^18.0.15",
    "@yarnpkg/doctor": "^3.1.3",
    "axios": "^0.27.2",
    "jsbi": "^4.3.0",
    "react": "^18.2.0",
    "react-query": "^3.39.1",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "@aptosis/aptos-api": "^0.0.2",
    "@aptosis/aptos-client": "^0.0.2",
    "@aptosis/seacliff": "workspace:^",
    "@movingco/aptos": "^0.1.51",
    "@movingco/core": "^0.1.51",
    "@movingco/prelude": "^0.1.51",
    "@omnimask/provider-interface": "^0.1.4",
    "@saberhq/tuple-utils": "^1.13.32",
    "axios": "^0.27.2",
    "tiny-invariant": "^1.2.0",
    "tslib": "^2.4.0",
    "unstated-next": "^1.1.0"
  },
  "peerDependencies": {
    "axios": "^0.27",
    "jsbi": "^4.3.0",
    "react": "*",
    "react-query": "*"
  }
}
