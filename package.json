{
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "name": "@aptosis/react-aptos-workspace",
  "version": "0.1.21",
  "description": "React library for interacting with the Aptos blockchain.",
  "repository": "git@github.com:aptosis/react-aptos.git",
  "author": "Ian Macalinao <ian@aptos.is>",
  "license": "Apache-2.0",
  "type": "module",
  "keywords": [
    "react",
    "wallet",
    "blockchain",
    "aptos",
    "move"
  ],
  "homepage": "https://aptos.is",
  "scripts": {
    "build": "yarn workspaces foreach --exclude @aptosis/react-aptos-workspace -vpti --topological-dev run build",
    "clean": "yarn workspaces foreach --exclude @aptosis/react-aptos-workspace -vpti --topological-dev run clean",
    "publish:all": "yarn workspaces foreach --exclude @aptosis/react-aptos-workspace -vpti --topological-dev npm publish",
    "docs:generate": "typedoc .",
    "bump": "yarn workspaces foreach -t version",
    "test": "jest",
    "lint": "eslint . --cache",
    "lint:ci": "eslint . --max-warnings=0",
    "typecheck": "tsc --build",
    "prepare": "husky install"
  },
  "packageManager": "yarn@3.2.1",
  "lint-staged": {
    "*.{ts,tsx,cjs}": "eslint --cache --fix",
    "*.{json,js,jsx,html,css,md,yml,yaml}": "prettier --write"
  },
  "devDependencies": {
    "@movingco/prelude": "^0.1.54",
    "@rushstack/eslint-patch": "^1.1.4",
    "@saberhq/eslint-config": "^2.0.2",
    "@saberhq/eslint-config-react": "^2.0.2",
    "@saberhq/tsconfig": "^2.0.2",
    "@types/eslint": "^8.4.5",
    "@types/jest": "^28.1.6",
    "@types/node": "^18.0.5",
    "@types/prettier": "^2.6.3",
    "@types/react": "^18.0.15",
    "@typescript-eslint/utils": "^5.30.6",
    "@yarnpkg/doctor": "^3.1.3",
    "aptos": "^1.2.0",
    "axios": "^0.27.2",
    "eslint": "^8.20.0",
    "husky": "^8.0.1",
    "jsbi": "^4.3.0",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "react": "^18.2.0",
    "react-query": "^3.39.2",
    "typedoc": "^0.23.7",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "@movingco/aptos": "^0.1.54",
    "@movingco/core": "^0.1.54",
    "@movingco/prelude": "^0.1.54",
    "@omnimask/provider-interface": "^0.1.7",
    "@saberhq/tuple-utils": "^1.13.32",
    "axios": "^0.27.2",
    "tiny-invariant": "^1.2.0",
    "tslib": "^2.4.0",
    "unstated-next": "^1.1.0"
  },
  "peerDependencies": {
    "jsbi": "^4.3.0",
    "react": "*",
    "react-query": "*"
  },
  "resolutions": {
    "axios": "^0.27"
  }
}
