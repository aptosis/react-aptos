{
  "name": "@aptosis/react-aptos",
  "version": "0.1.0",
  "description": "React library for interacting with the Aptos blockchain.",
  "repository": "git@github.com:aptosis/react-aptos.git",
  "author": "Ian Macalinao <ian@moving.wtf>",
  "license": "AGPL-3.0",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "keywords": [
    "react",
    "wallet",
    "blockchain",
    "aptos",
    "move"
  ],
  "homepage": "https://aptos.is",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "files": [
    "dist/",
    "src/"
  ],
  "publishConfig": {
    "access": "public"
  },
  "bugs": {
    "url": "https://github.com/Omnimask/provider-interface/issues",
    "email": "ian@moving.wtf"
  },
  "scripts": {
    "build": "tsc --project tsconfig.esm.json && tsc --project tsconfig.cjs.json",
    "clean": "rm -fr dist/",
    "lint": "eslint . --cache",
    "lint:ci": "eslint . --max-warnings=0",
    "lint:fix": "eslint . --fix",
    "docs:generate": "typedoc --excludePrivate --includeVersion --out site/ src/index.ts",
    "prepare": "husky install"
  },
  "packageManager": "yarn@3.2.1",
  "lint-staged": {
    "*.{ts,tsx,cjs}": "eslint --cache --fix",
    "*.{json,js,jsx,html,css,md,yml,yaml}": "prettier --write"
  },
  "devDependencies": {
    "@movingco/prelude": "^0.1.36",
    "@rushstack/eslint-patch": "^1.1.4",
    "@saberhq/eslint-config": "^1.13.32",
    "@saberhq/tsconfig": "^1.13.32",
    "@types/eslint": "^8.4.3",
    "@types/node": "^18.0.0",
    "@types/prettier": "^2.6.3",
    "@types/react": "^18.0.14",
    "@yarnpkg/doctor": "^3.1.3",
    "aptos": "^1.2.0",
    "axios": "^0.27.2",
    "eslint": "^8.18.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "react": "^18.2.0",
    "react-query": "^3.39.1",
    "typedoc": "^0.22.18",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "@movingco/aptos": "^0.1.36",
    "@movingco/aptos-api": "^0.1.36",
    "@movingco/aptos-coin": "^0.1.36",
    "@movingco/core": "^0.1.36",
    "@omnimask/provider-interface": "^0.1.0",
    "@saberhq/tuple-utils": "^1.13.31",
    "@types/lodash.groupby": "^4.6.7",
    "@types/lodash.keyby": "^4.6.7",
    "@vespaiach/axios-fetch-adapter": "^0.3.0",
    "axios": "^0.27.2",
    "eventemitter3": "^4.0.7",
    "lodash.groupby": "^4.6.0",
    "lodash.keyby": "^4.6.0",
    "tiny-invariant": "^1.2.0",
    "tslib": "^2.4.0",
    "unstated-next": "^1.1.0"
  },
  "peerDependencies": {
    "@movingco/prelude": "*",
    "aptos": "^0.27",
    "bn.js": "^5",
    "jsbi": "^4.3.0",
    "react": "*",
    "react-query": "*"
  },
  "resolutions": {
    "axios": "^0.27"
  }
}
